<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_Menu"
    android:background="@drawable/main_menu">


    <ImageView
        android:id="@+id/imV_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/svg_main_menu" />

    <ImageButton
        android:id="@+id/im_btn_1_menu"
        android:layout_width="175dp"
        android:layout_height="188.1dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#91847E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imV_logo"
        app:srcCompat="@drawable/btn_menu1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/im_btn_2_menu"
        android:layout_width="175dp"
        android:layout_height="188.1dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#91847E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/im_btn_1_menu"
        app:layout_constraintTop_toBottomOf="@+id/imV_logo"
        app:srcCompat="@drawable/btn_menu2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/im_btn_3_menu"
        android:layout_width="175dp"
        android:layout_height="188.1dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="1dp"
        android:backgroundTint="#91847E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/im_btn_1_menu"
        app:srcCompat="@drawable/btn_menu3"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/im_btn_4_menu"
        android:layout_width="175dp"
        android:layout_height="188.1dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="19dp"
        android:backgroundTint="#91847E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/im_btn_3_menu"
        app:layout_constraintTop_toBottomOf="@+id/im_btn_2_menu"
        app:srcCompat="@drawable/btn_menu4"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/im_btn_5_menu"
        android:layout_width="175dp"
        android:layout_height="188.1dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="#91847E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/im_btn_3_menu"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_menu5"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/im_btn_6_menu"
        android:layout_width="175dp"
        android:layout_height="188.1dp"
        android:layout_marginStart="6dp"
        android:backgroundTint="#91847E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toEndOf="@+id/im_btn_5_menu"
        app:layout_constraintTop_toBottomOf="@+id/im_btn_4_menu"
        app:layout_constraintVertical_bias="0.016"
        app:srcCompat="@drawable/btn_menu6"
        tools:ignore="SpeakableTextPresentCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>